Configs: {
	TweaksConfig: {
		safeNinja: {
			Tooltip:
				'''
				スライムニンジャ(通常の忍者NPCではない)が通常およびスパイク付きスライムから受けるダメージを25%減少させる
				キングスライムが倒されたときにドロップするスライムによってニンジャが殺されるのを防ぐ
				'''
			Label: "スライムニンジャの安全確保"
		}

		flasksAffectThrowingWeapons: {
			Tooltip:
				'''
				バニラのフラスコがその効果を投擲ダメージを持つ武器に適用できるようにする
				アイテムのツールチップも(バフのツールチップではなく)変更に合わせて調整する
				MODで追加されたフラスコには影響しない
				'''
			Label: "フラスコが投擲武器に影響する"
		}

		moveThrowingKnives: {
			Tooltip:
				'''
				商人NPCの店から投げナイフを削除し、忍者NPCが販売するようにする
				忍者は商人のようにブラッドムーン時だけでなく、いつでも夜に投げナイフを販売する点に注意
				'''
			Label: "商人から投げナイフを削除"
		}

		moveShurikens: {
			Tooltip: "商人NPCの店から手裏剣を削除し、忍者NPCが販売するようにする"
			Label: "商人から手裏剣を削除"
		}

		moveNinjaArmor: {
			Tooltip:
				'''
				キングスライムのドロップ品から忍者防具を削除し、忍者NPCが販売できるようにする
				防具の売価もそれぞれ銀貨15枚にわずかに調整する
				参考までに、頭巾は通常銀貨20枚、シャツとパンツは銀貨10枚で売れる
				'''
			Label: "忍者防具を忍者NPCに移動"
		}

		visiblyFreeNinja: {
			Tooltip:
				'''
				キングスライムの描画を調整し、忍者NPCが最初に解放された後、彼の中に閉じ込められた忍者を削除する
				忍者NPCがアンロックされていないワールドでは効果がない
				他のMODもキングスライムの描画方法を変更している場合、奇妙な(視覚的な)効果があるかもしれない点に注意
				'''
			Label: "解放された忍者を視覚的に表示"
		}

		DisplayName: "その他のバニラ調整"
	}

	ConversionConfig: {
		globalConversion: {
			Tooltip:
				'''
				全ての消耗品遠距離武器(バニラおよびMODで追加されたもの両方)を自動的に投擲クラスに変換する
				バニラのスノーボールのように、弾薬としてもカウントされるアイテムは変換しない
				タイプ変更に対応するためのステータス調整は行われないため、バランスに影響を与える可能性がある
				この設定は、以下の多くの独立した変換設定を上書きする
				'''
			Label: "全ての消耗品遠距離武器を変換"
		}

		daggerfishConv: {
			Tooltip: "この武器のダメージクラスを投擲に変更する"
			Label: "[i:3197] フロストダガーフィッシュを元に戻す"
